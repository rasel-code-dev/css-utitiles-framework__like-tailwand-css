{
  "name": "sql-social-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "build-utilities--classes-compress": "sass ./src/utilities-classes/**.scss ./src/utilities-classes/index.css --style compressed",
    "build-utilities--classes": "sass --watch ./src/utilities-classes/index.scss ./src/utilities-classes/index.css",
    "mk_js-to-scss": "nodemon ./src/variables/makeScss.js",
    "dev-css": "npm-run-all --parallel mk_js-to-scss build-utilities--classes",
    "build-css": "npm-run-all --parallel mk_js-to-scss build-utilities--classes-compress"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "preact": "^10.5.13",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "sass": "^1.43.4"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@preact/preset-vite": "^2.0.0",
    "vite": "^2.6.4"
  }
}
